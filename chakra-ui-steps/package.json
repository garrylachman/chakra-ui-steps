{
  "version": "2.0.1",
  "license": "MIT",
  "name": "chakra-ui-steps",
  "author": "Jean Verster",
  "keywords": [
    "steps",
    "stepper",
    "chakra",
    "navigation",
    "multi"
  ],
  "main": "./dist/chakra-ui-steps.umd.js",
  "module": "./dist/chakra-ui-steps.es.js",
  "types": "dist/index.d.ts",
  "description": "Steps component designed to work seamlessly with Chakra UI",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/chakra-ui-steps.es.js",
      "require": "./dist/chakra-ui-steps.umd.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite build --watch",
    "prepare": "yarn build",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "vitest"
  },
  "peerDependencies": {
    "@babel/core": "7.17.12",
    "@babel/plugin-transform-typescript": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@babel/types": "7.17.12",
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "framer-motion": "^7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "homepage": "https://github.com/jeanverster/chakra-ui-steps",
  "devDependencies": {
    "@babel/core": "7.17.12",
    "@babel/plugin-transform-typescript": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@babel/runtime": "7.17.7",
    "@babel/types": "7.17.12",
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@rollup/plugin-typescript": "^9.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^2.2.0",
    "framer-motion": "^7",
    "husky": "^7.0.4",
    "jsdom": "^20.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.0",
    "semantic-release-monorepo": "^7.0.5",
    "storybook": "^6.5.9",
    "storybook-addon-performance": "^0.16.1",
    "tslib": "^2.4.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.25.2"
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git"
    ],
    "extends": "semantic-release-monorepo"
  },
  "dependencies": {
    "@storybook/builder-vite": "^0.2.5"
  }
}
